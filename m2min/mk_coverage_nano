./artifacts/coverage.exe ./M2_nano_test.js artifacts/M2_nano_test.M1
gcovr artifacts/ --html-details -o artifacts/example.html

~/ffnew/firefox/firefox artifacts/example.html

sha256sum -c sums_nano_test
